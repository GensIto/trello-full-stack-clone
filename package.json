  "scripts": {
    "build": "tsc -b && vite build",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "vite",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "test": "vitest",
    "db:gen": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply torello-full-stack-clone --local",
    "db:migrate:remote": "wrangler d1 migrations apply torello-full-stack-clone --remote",
    "db:studio": "drizzle-kit studio --config drizzle.config.local.ts",
    "db:studio:prod": "drizzle-kit studio"
  },
  "type": "module"
}
